<h1>ClientUsers#edit</h1>
<p>Find me in app/views/client_users/edit.html.erb</p>

	<div class="col-xs-6">
		<%= form_with(model:[@client_user], local: true) do |f| %>
			<table>
				<tr>
					<td>名前</td>
					<td>(姓)　<%= f.text_field  :family_name %></td>
					<td>(名)　<%= f.text_field  :first_name %></td>
				</tr>
				<tr>
					<td>フリガナ</td>
					<td>(セイ)<%= f.text_field  :kana_family_name %></td>
					<td>(メイ)<%= f.text_field  :kana_first_name %></td>
				</tr>
				<tr>
					<td>メールアドレス</td>
					<td><%= f.text_field  :email %></td>
					<td></td>
				</tr>
				<tr>
					<td>郵便番号(ハイフンなし)</td>
					<td><%= f.text_field  :postal_code %></td>
					<td></td>
				</tr>
				<tr>
					<td>住所</td>
					<td><%= f.text_field  :address %></td>
				</tr>
				<tr>
					<td>電話番号</td>
					<td><%= f.text_field  :tel %></td>
					<td></td>
				</tr>
			</table>
			<%= f.submit "変更する" %>
		<% end %>
	</div>
