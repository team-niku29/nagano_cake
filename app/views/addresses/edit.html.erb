<div class= "container">
  <div class='row'>
    <%= form_with model: [@client_user, @address] do |f| %>
    <p>配送先編集</p>
    <table>
      <tr>
        <td>郵便番号(ハイフンなし)</td>
        <td><%= f.text_field :postal_code %></td>
      </tr>
      <tr>
        <td>住所</td>
        <td><%= f.text_field :address %></td>
      </tr>
      <tr>
        <td>宛名</td>
        <td><%= f.text_field :name %></td>
      </tr>
    </table>
    <%= f.submit '編集する', class:"btn btn-primary" %>
    <% end %>
  </div>
</div>