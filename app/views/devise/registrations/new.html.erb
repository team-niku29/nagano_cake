
  <div class= "row center">
    <div class="col-md-8 col-md-offset-2">
      <h2 class="page-header text-center">新規会員登録</h2>
      <div class="form-group">
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      </div>
      <%= render "devise/shared/error_messages", resource: resource %>
        <table>
          <tr>
            <td>
              <div class= "form-group">
                  <%= f.label :"(性) " %>            
                  <%= f.text_field :family_name, class: 'form-control',autofocus: true, autocomplete: "family_name" %>
              </div>
            </td>
            <td>　</td>
            <td>
              <div class='form-group'>
                <%= f.label :"(名) " %>
                <%= f.text_field :first_name, class: 'form-control',autofocus: true, autocomplete: "first_name" %>
              </div>
            </td>
          </tr>
        </table>
        <table>
          <tr>
            <td>
              <div class='form-group '>
                <%= f.label :"(セイ)" %>
                <%= f.text_field :kana_family_name, class: 'form-control',autofocus: true, autocomplete: "kana_famly_name" %>
              </div>
            </td>
              <td>　</td>
            <td>
              <div class='form-group'>
                <%= f.label :"(メイ)" %>
                <%= f.text_field :kana_first_name, class: 'form-control',autofocus: true, autocomplete: "kana_first_name" %>
              </div>
            </td>
            <td>
          </tr>
        </table>
        <div class="form-group">
          <%= f.label :"郵便番号(ハイフンなし)" %>
          <%= f.text_field :postal_code, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :"メールアドレス" %>
          <%= f.email_field :email,class: 'form-control', autofocus: true, autocomplete: "email" %>
        </div>
        <div class="form-group">
          <%= f.label :"  ご住所情報（マンション名まで)" %>
          <%= f.text_field :address,class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :"電話番号(ハイフンなし)" %>
          <%= f.text_field :tel,class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :"password" %>
          <% if @minimum_password_length %>
          <em>(<%= @minimum_password_length %> 文字以上)</em>
          <% end %><br />
          <%= f.password_field :password,class: 'form-control', autocomplete: "new-password" %>
        </div>
        <div class="form-group">
          <%= f.label :"password(確認用)" %><br />
          <%= f.password_field :password_confirmation,class: 'form-control', autocomplete: "new-password" %>
        </div>
          <div class="actions">
            <%= f.submit "新規登録" ,class: "btn btn-primary center  col-md-offset-5" %>
          </div>
      <% end %>
      <div class= "text-center">
        <h3>既に登録済みの方</h3>
        <h5><%= link_to("こちら", new_client_user_session_path) %>からログインください<h5>
      </div>
    </div>
    <div class= col-md-2>
    </div>